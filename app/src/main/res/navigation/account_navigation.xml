<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/account_navigation"
    app:startDestination="@id/loginFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="digitized.gamehub.views.LoginFragment"
        android:label="LoginFragment" >
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment_to_gameCardsFragment2"
            app:destination="@id/gameCardsFragment2" />
    </fragment>
    <fragment
        android:id="@+id/accountSettingsFragment2"
        android:name="digitized.gamehub.views.AccountSettingsFragment"
        android:label="AccountSettingsFragment" >
        <action
            android:id="@+id/action_accountSettingsFragment2_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="digitized.gamehub.views.RegisterFragment"
        android:label="RegisterFragment" >
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_registerFragment_to_gameCardsFragment2"
            app:destination="@id/gameCardsFragment2" />
    </fragment>
    <fragment
        android:id="@+id/gameCardsFragment2"
        android:name="digitized.gamehub.views.GameCardsFragment"
        android:label="GameCardsFragment" />
</navigation>